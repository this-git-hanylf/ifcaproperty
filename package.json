    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "moment": "^2.29.1",
    "native-base": "^3.0.3",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-easy-grid": "^0.2.2",
